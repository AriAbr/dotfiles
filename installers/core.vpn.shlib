install(){
	if ! command -v startctui &>/dev/null; then
		TEMP_DIR=vpn_tmp
		mkdir $TEMP_DIR

		sudo apt update -v
		sudo apt install default-jdk -y
		TAR=assets/ConnectTunnel_Linux64-12.42.00664.tar
		tar -xvf $TAR -C ./$TEMP_DIR
		cd $TEMP_DIR
		yes | sudo ./install.sh
		cd ..
		rm -rf $TEMP_DIR
	fi
}
